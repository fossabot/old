    "glob": "^7.1.4",
    "infer-owner": "^1.0.4",
    "lru-cache": "^6.0.0",
    "minipass": "^3.1.1",
    "minipass-collect": "^1.0.2",
    "minipass-flush": "^1.0.5",
    "minipass-pipeline": "^1.2.2",
    "mkdirp": "^1.0.3",
    "p-map": "^4.0.0",
    "promise-inflight": "^1.0.1",
    "rimraf": "^3.0.2",
    "ssri": "^8.0.1",
    "tar": "^6.0.2",
    "unique-filename": "^1.1.1"
  },
  "devDependencies": {
    "@npmcli/lint": "^1.0.1",
    "benchmark": "^2.1.4",
